<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duk.lab.rxandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".basic.RxAndroidBasicActivity"
            android:label="@string/title_activity_rx_android_basic"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".basic.HelloRxLifecycleActivity"
            android:label="@string/title_activity_hello_rx_lifecycle" />
        <activity android:name=".basic.SearchActivity" />
        <activity
            android:name=".application.RecyclerActivity"
            android:label="@string/title_activity_recycler"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>